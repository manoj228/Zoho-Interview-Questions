1230 0130
1200 0100
1600 1700

#include <bits/stdc++.h>
using namespace std;

int main() {
	int n ;
	cin >> n;
	
	vector<int> a(n) , b(n);
	int c=0;
	for(int i=0; i<n; i++) cin >> a[i] >> b[i] ;
	
	
	for(int i=0; i<n; i++){
		if(a[i] > b[i]) c++;
	}
	
	cout << c << endl;
	
	return 0;
}
